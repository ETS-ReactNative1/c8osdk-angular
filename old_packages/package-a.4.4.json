{
  "name": "c8osdkangular",
  "version": "2.1.56",
  "description": "convertigo's MBaaS client API service",
  "main": "bundle/index.umd.js",
  "module": "src/index.js",
  "typings": "src/index.d.ts",
  "files": [
    "src/",
    "LICENCE",
    "tsconfig.json"
  ],
  "scripts": {
    "test": "karma start",
    "test-watch": "karma start --singleRun=false --autoWatch=true --captureExceptions",
    "clean": "rimraf bundles coverage src/**/*.d.ts src/**/*.metadata.json src/**/*.js src/**/*.js.map src/**/*.ngsummary.json",
    "prepare": "npm run clean && ngc && npm run build",
    "build": "webpack && cp bundles/index.umd.js bundles/index.js",
    "docs": "npm run typedoc -- --options typedoc.json --exclude '**/*.spec.ts' ./src/c8o/",
    "typedoc": "typedoc",
    "build-dev": "tsc -p ."
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/convertigo/C8oSDKAngular"
  },
  "keywords": [
    "MBaaS",
    "TypeScript",
    "Angular",
    "Ionic 2",
    "Convertigo",
    "mbaas",
    "madp",
    "mobility",
    "nosql",
    "couchdb"
  ],
  "author": {
    "name": "Convertigo",
    "email": "info@convertigo.com",
    "url": "http://www.convertigo.com/"
  },
  "license": "Apache License Version 2.0, January 2004",
  "dependencies": {
    "@angular/common": "4.4.4",
    "@angular/compiler": "4.4.4",
    "@angular/compiler-cli": "4.4.4",
    "@angular/core": "4.4.4",
    "@angular/forms": "4.4.4",
    "@angular/http": "4.4.4",
    "@angular/platform-browser": "4.4.4",
    "@angular/platform-browser-dynamic": "4.4.4",
    "@types/pouchdb": "6.3.1",
    "pouchdb-browser": "6.3.4",
    "rxjs": "5.5.2",
    "sw-toolbox": "3.6.0",
    "zone.js": "0.8.18"
  },
  "devDependencies": {
    "rimraf": "2.6.1",
    "@types/jasmine": "2.8.2",
    "awesome-typescript-loader": "3.4.1",
    "codelyzer": "4.0.1",
    "commitizen": "2.9.6",
    "concurrently": "3.5.1",
    "core-js": "2.5.1",
    "istanbul-instrumenter-loader": "3.0.0",
    "jasmine-core": "2.8.0",
    "karma": "1.7.1",
    "karma-chrome-launcher": "2.2.0",
    "karma-coverage": "1.1.1",
    "karma-jasmine": "1.1.1",
    "karma-mocha-reporter": "2.2.5",
    "karma-remap-coverage": "0.1.4",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "2.0.6",
    "loader-utils": "1.1.0",
    "reflect-metadata": "0.1.10",
    "source-map-loader": "0.2.3",
    "ts-helpers": "1.1.2",
    "tsify": "3.0.4",
    "tslint": "5.8.0",
    "tslint-loader": "3.5.3",
    "typedoc": "0.9.0",
    "webpack": "3.10.0",
    "typescript": "2.4.2"
  },
  "peerDependencies": {
    "@angular/common": "~4.4.4",
    "@angular/compiler": "~4.4.4",
    "@angular/compiler-cli": "~4.4.4",
    "@angular/core": "~4.4.4",
    "@angular/forms": "~4.4.4",
    "@angular/http": "~4.4.4",
    "@angular/platform-browser": "~4.4.4",
    "@angular/platform-browser-dynamic": "~4.4.4",
    "rxjs": "~5.4.3",
    "zone.js": "~0.8.17",
    "sw-toolbox": "~3.6.0"
  },
  "cordovaPlatforms": [],
  "angularCompilerOptions": {
    "strictMetadataEmit": true,
    "skipTemplateCodegen": true
  }
}
