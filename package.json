{
  "name": "c8osdkangular",
  "version": "2.1.43-patch20180611",
  "description": "convertigo's MBaaS client API service",
  "main": "bundle/index.umd.js",
  "module": "src/index.js",
  "typings": "src/index.d.ts",
  "files": [
    "src/",
    "LICENCE",
    "tsconfig.json"
  ],
  "scripts": {
    "test": "karma start",
    "test-watch": "karma start --singleRun=false --autoWatch=true --captureExceptions",
    "clean": "rimraf bundles coverage src/**/*.d.ts src/**/*.metadata.json src/**/*.js src/**/*.js.map src/**/*.ngsummary.json",
    "prepublish": "ngc && npm run build",
    "build": "webpack && cp bundles/index.umd.js bundles/index.js",
    "postbuild": "find ./bundles -type f -exec sed -i 's/pouchdb_browser_1.default/pouchdb_browser_1/g' {} + && find ./src -type f -exec sed -i 's/new pouchdb_browser_1.default/new pouchdb_browser_1/g' {} +",
    "docs": "npm run typedoc -- --options typedoc.json --exclude '**/*.spec.ts' ./src/c8o/",
    "typedoc": "typedoc",
    "build-dev": "tsc -p ."
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/convertigo/C8oSDKAngular"
  },
  "keywords": [
    "MBaaS",
    "TypeScript",
    "Angular",
    "Ionic 2",
    "Convertigo",
    "mbaas",
    "madp",
    "mobility",
    "nosql",
    "couchdb"
  ],
  "author": {
    "name": "Convertigo",
    "email": "info@convertigo.com",
    "url": "http://www.convertigo.com/"
  },
  "license": "Apache License Version 2.0, January 2004",
  "dependencies": {
    "@angular/common": "4.1.3",
    "@angular/compiler": "4.1.3",
    "@angular/compiler-cli": "4.1.3",
    "@angular/core": "4.1.3",
    "@angular/forms": "4.1.3",
    "@angular/http": "4.1.3",
    "@angular/platform-browser": "4.1.3",
    "@angular/platform-browser-dynamic": "4.1.3",
    "rxjs": "5.4.0",
    "sw-toolbox": "3.6.0",
    "zone.js": "0.8.12",
    "@types/pouchdb": "6.1.1",
    "pouchdb-browser": "6.2.0"
  },
  "devDependencies": {
    "@types/jasmine": "2.5.52",
    "awesome-typescript-loader": "3.1.3",
    "codelyzer": "3.1.1",
    "commitizen": "2.9.6",
    "concurrently": "3.4.0",
    "core-js": "2.4.1",
    "istanbul-instrumenter-loader": "2.0.0",
    "jasmine-core": "2.6.4",
    "karma": "1.7.0",
    "karma-chrome-launcher": "2.1.1",
    "karma-coverage": "1.1.1",
    "karma-jasmine": "1.1.0",
    "karma-mocha-reporter": "2.2.3",
    "karma-remap-coverage": "0.1.4",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "2.0.3",
    "loader-utils": "1.1.0",
    "reflect-metadata": "0.1.10",
    "source-map-loader": "0.2.1",
    "ts-helpers": "1.1.2",
    "tsify": "3.0.1",
    "tslint": "5.4.3",
    "tslint-loader": "3.5.3",
    "typedoc": "0.7.1",
    "typings": "2.1.1",
    "webpack": "3.0.0",
    "typescript": "2.3.4"
  },
  "cordovaPlatforms": [],
  "angularCompilerOptions": {
    "strictMetadataEmit": true,
    "skipTemplateCodegen": true
  }
}
