{
  "name": "c8osdkangular",
  "version": "2.1.25",
  "description": "convertigo's MBaaS client API service",
  "main": "bundle/index.umd.js",
  "module": "src/index.js",
  "typings": "src/index.d.ts",
  "files": [
    "src/",
    "LICENCE",
    "tsconfig.json"
  ],
  "scripts": {
    "test": "karma start",
    "test-watch": "karma start --singleRun=false --autoWatch=true --captureExceptions",
    "clean": "rimraf bundles coverage src/**/*.d.ts src/**/*.metadata.json src/**/*.js src/**/*.js.map src/**/*.ngsummary.json",
    "prepublish": "ngc && npm run build",
    "build": "webpack && cp bundles/index.umd.js bundles/index.js",
    "postbuild": "find ./bundles -type f -exec sed -i 's/pouchdb_browser_1.default/pouchdb_browser_1/g' {} + && find ./src -type f -exec sed -i 's/new pouchdb_browser_1.default/new pouchdb_browser_1/g' {} +",
    "docs": "npm run typedoc -- --options typedoc.json --exclude '**/*.spec.ts' ./src/c8o/",
    "typedoc": "typedoc",
    "build-dev" : "tsc -p ."
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/convertigo/C8oSDKAngular"
  },
  "keywords": [
    "MBaaS",
    "TypeScript",
    "Angular",
    "Ionic 2",
    "Convertigo",
    "mbaas",
    "madp",
    "mobility",
    "nosql",
    "couchdb"
  ],
  "author": {
    "name": "Convertigo",
    "email": "info@convertigo.com",
    "url": "http://www.convertigo.com/"
  },
  "license": "Apache License Version 2.0, January 2004",
  "dependencies": {
    "@angular/common": "4.1.2",
    "@angular/compiler": "4.1.2",
    "@angular/compiler-cli": "4.1.2",
    "@angular/core": ">=4.1.2",
    "@angular/forms": "4.1.2",
    "@angular/http": "4.1.2",
    "@angular/platform-browser": "4.1.2",
    "@angular/platform-browser-dynamic": "4.1.2",
    "rxjs": "5.1.1",
    "sw-toolbox": "3.6.0",
    "zone.js": "0.8.11",
    "@types/pouchdb": "5.4.28",
    "pouchdb-browser": "^6.1.1"
  },
  "devDependencies": {
    "@angular/common": "4.1.2",
    "@angular/compiler": "4.1.2",
    "@angular/compiler-cli": "4.1.2",
    "@angular/core": "4.1.2",
    "@angular/forms": "4.1.2",
    "@angular/http": "4.1.2",
    "@angular/platform-browser": "4.1.2",
    "@angular/platform-browser-dynamic": "4.1.2",
    "@types/hammerjs": "2.0.33",
    "@types/jasmine": "2.5.38",
    "@types/node": "6.0.52",
    "awesome-typescript-loader": "3.0.0-beta.17",
    "browserify": "^13.1.0",
    "codelyzer": "2.0.0-beta.3",
    "commitizen": "2.9.2",
    "concurrently": "^3.0.0",
    "core-js": "2.4.1",
    "cz-conventional-changelog": "1.2.0",
    "istanbul-instrumenter-loader": "0.2.0",
    "jasmine-core": "2.5.2",
    "karma": "1.3.0",
    "karma-chrome-launcher": "2.0.0",
    "karma-coverage": "1.1.1",
    "karma-firefox-launcher": "^1.0.0",
    "karma-jasmine": "1.0.2",
    "karma-mocha-reporter": "^2.1.0",
    "karma-remap-coverage": "0.1.2",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "1.8.0",
    "loader-utils": "0.2.16",
    "reflect-metadata": "0.1.8",
    "semantic-release": "6.3.2",
    "source-map-loader": "0.1.5",
    "ts-helpers": "1.1.2",
    "tsify": "^1.0.7",
    "tslint": "4.0.2",
    "tslint-loader": "3.3.0",
    "typedoc": "^0.5.10",
    "typings": "^1.4.0",
    "webpack": "2.1.0-beta.27",
    "typescript": "2.3.3"
  },
  "cordovaPlatforms": [],
  "angularCompilerOptions": {
    "strictMetadataEmit": true,
    "skipTemplateCodegen": true
  }
}
